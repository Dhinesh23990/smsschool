<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.tgi.sd.domain.AttendanceSummaryVO" table="attendancesummary">
		<cache usage="read-write"/> 
		
		<id name="id" type="java.lang.String">
			<column name="Id" />
		</id>
		
		<property name="day" type="java.util.Date">
		 	<column name="day" />
		</property>
		
		<property name="fullDay" type="java.lang.Boolean">
		 	<column name="fullDay" />
		</property>
		
		<property name="morning" type="java.lang.Boolean">
		 	<column name="morning" />
		</property>
		
		<property name="afternoon" type="java.lang.Boolean">
		 	<column name="afternoon" />
		</property>
		
		<property name="note" type="java.lang.String">
		 	<column name="note" />
		</property>
		
		<property name="attendancePercenatge" type="java.lang.Long">
		 	<column name="attendancePercenatge" />
		</property>
		
		<property name="presentDays" type="java.lang.Integer">
		 	<column name="presentDays" />
		</property>
		
		<property name="totalDays" type="java.lang.Integer">
		 	<column name="totalDays" />
		</property>
		
		<property name="standard" type="java.lang.String">
		 	<column name="standard" />
		</property>
		
		<property name="admissionNumber" type="java.lang.String">
		 	<column name="admissionNumber" />
		</property>
		
		<property name="studentName" type="java.lang.String">
		 	<column name="studentName" />
		</property>
		
		<property name="section" type="java.lang.String">
		 	<column name="section" />
		</property>
		
		<property name="schoolId" type="java.lang.String">
		 	<column name="schoolId" />
		</property>
	</class>
</hibernate-mapping>