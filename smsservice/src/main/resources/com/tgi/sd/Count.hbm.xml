<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.tgi.sd.domain.CountVO" table="count">
		<cache usage="read-write"/> 
		
		<id name="id" type="java.lang.String">
			<column name="Id" />
		</id>
		
		<property name="boysCount" type="java.lang.Integer">
		 	<column name="boysCount" />
		</property>
		
		<property name="girlssCount" type="java.lang.Integer">
		 	<column name="girlssCount" />
		</property>
		
		<property name="totalCount" type="java.lang.Integer">
		 	<column name="totalCount" />
		</property>
		
		<property name="className" type="java.lang.String">
		 	<column name="className" />
		</property>
		
		<property name="section" type="java.lang.String">
		 	<column name="section" />
		</property>
		
		<property name="schoolId" type="java.lang.String">
		 	<column name="schoolId" />
		</property>
	</class>
</hibernate-mapping>