<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.tgi.sd.domain.EventVO" table="event">
		<cache usage="read-write"/> 
		
		<id name="id" type="java.lang.String">
			<column name="Id" />
		</id>
		
		<property name="eventName" type="java.lang.String">
		 	<column name="eventName" />
		</property>
		
		<property name="description" type="java.lang.String">
		 	<column name="description" />
		</property>
		
		<property name="venue" type="java.lang.String">
		 	<column name="venue" />
		</property>
		
		<property name="time" type="java.lang.String">
		 	<column name="time" />
		</property>
		
		<property name="eventStartDate" type="java.util.Date">
		 	<column name="eventStartDate" />
		</property>
		
		<property name="eventEndDate" type="java.util.Date">
		 	<column name="eventEndDate" />
		</property>
		
		
		<property name="schoolId" type="java.lang.String">
			<column name="schoolId" />
		</property>
		
	</class>
</hibernate-mapping>