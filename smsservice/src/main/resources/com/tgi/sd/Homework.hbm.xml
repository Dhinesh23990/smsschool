<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.tgi.sd.domain.HomeWorkVO" table="homework">
		<cache usage="read-write"/> 
		
		<id name="id" type="java.lang.String">
			<column name="Id" />
		</id>
		
		<property name="studentId" type="java.lang.String">
		 	<column name="studentId" />
		</property>
		
		<property name="subject" type="java.lang.String">
		 	<column name="subject" />
		</property>
		
		<property name="message" type="java.lang.String">
		 	<column name="message" />
		</property>
		
		<property name="schoolId" type="java.lang.String">
		 	<column name="schoolId" />
		</property>
		
		<property name="startDate" type="java.util.Date">
		 	<column name="startDate" />
		</property>
		
		<property name="endDate" type="java.util.Date">
		 	<column name="endDate" />
		</property>
		
		<property name="classIds" type="java.lang.String">
			<column name="classIds" />
		</property>
		
		<property name="studentIds" type="java.lang.String">
			<column name="studentIds" />
		</property>
		
		<property name="HomeWorkName" type="java.lang.String">
			<column name="HomeWorkName" />
		</property>
		
	</class>
</hibernate-mapping>