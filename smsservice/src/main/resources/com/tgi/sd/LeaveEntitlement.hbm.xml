<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.tgi.sd.domain.LeaveEntitlementVO" table="leaveentitlement">
		<cache usage="read-write"/> 
		
		<id name="id" type="java.lang.String">
			<column name="Id" />
		</id>
		
		<property name="leaveMasterId" type="java.lang.String">
		 	<column name="leaveMasterId" />
		</property>
		
		<property name="staffId" type="java.lang.Double">
		 	<column name="staffId" />
		</property>
		
		<property name="leaveCount" type="java.lang.Integer">
		 	<column name="leaveCount" />
		</property>
		
		<property name="leavePeriodId" type="java.lang.String">
		 	<column name="leavePeriodId" />
		</property>
		
		<property name="schoolId" type="java.lang.String">
		 	<column name="schoolId" />
		</property>
	</class>
</hibernate-mapping>