<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.tgi.sd.domain.LeaveMasterVO" table="leavemaster">
		<cache usage="read-write"/> 
		
		<id name="id" type="java.lang.String">
			<column name="Id" />
		</id>
		
		<property name="leaveName" type="java.lang.String">
		 	<column name="leaveName" />
		</property>
		
		<property name="leaveShortName" type="java.lang.String">
		 	<column name="leaveShortName" />
		</property>
		
		<property name="leaveCount" type="java.lang.Integer">
		 	<column name="leaveCount" />
		</property>
		
		<property name="leaveType" type="java.lang.String">
		 	<column name="leaveType" />
		</property>
		
		<property name="leaveFreq" type="java.lang.String">
		 	<column name="leaveFreq" />
		</property>
		
		<property name="carryNextCycle" type="java.lang.Boolean">
		 	<column name="carryNextCycle" />
		</property>
		
		<property name="schoolId" type="java.lang.String">
		 	<column name="schoolId" />
		</property>
	</class>
</hibernate-mapping>