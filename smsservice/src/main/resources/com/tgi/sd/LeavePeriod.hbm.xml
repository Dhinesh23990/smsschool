<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.tgi.sd.domain.LeavePeriodVO" table="leaveperiod">
		<cache usage="read-write"/> 
		
		<id name="id" type="java.lang.String">
			<column name="Id" />
		</id>
		
		<property name="periodName" type="java.lang.String">
		 	<column name="periodName" />
		</property>
		
		<property name="leaveMasterId" type="java.lang.String">
		 	<column name="leaveMasterId" />
		</property>
		
		<property name="leaveCount" type="java.lang.Integer">
		 	<column name="leaveCount" />
		</property>
		
		<property name="startDate" type="java.lang.String">
		 	<column name="startDate" />
		</property>
		
		<property name="endDate" type="java.lang.String">
		 	<column name="endDate" />
		</property>
		
		<property name="schoolId" type="java.lang.String">
		 	<column name="schoolId" />
		</property>
	</class>
</hibernate-mapping>