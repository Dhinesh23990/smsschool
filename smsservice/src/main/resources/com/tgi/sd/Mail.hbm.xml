<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.tgi.sd.domain.MailVO" table="mail">
		<cache usage="read-write"/> 
		
		<id name="id" type="java.lang.String">
			<column name="Id" />
		</id>
		
		<property name="mailType" type="java.lang.String">
		 	<column name="mailType" />
		</property>
		
		<property name="fromAddress" type="java.lang.String">
		 	<column name="fromAddress" />
		</property>
		
		<property name="toAddress" type="java.lang.String">
		 	<column name="toAddress" />
		</property>
		
		<property name="ccAddress" type="java.lang.String">
		 	<column name="ccAddress" />
		</property>
		
		<property name="bccAddress" type="java.lang.String">
		 	<column name="bccAddress" />
		</property>
		
		<property name="subject" type="java.lang.String">
		 	<column name="subject" />
		</property>
		
		<property name="mailContent" type="java.lang.String">
			<column name="mailContent" />
		</property>
		
		<property name="sentOn" type="java.lang.String">
			<column name="sentOn" />
		</property>
		
		<property name="retryCount" type="java.lang.Integer">
			<column name="retryCount" />
		</property>
		
		<property name="errorMailReport" type="java.lang.String">
			<column name="errorMailReport" />
		</property>
		
		<property name="status" type="java.lang.String">
			<column name="status" />
		</property>
		
	</class>
</hibernate-mapping>