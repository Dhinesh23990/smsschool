		<property name="parentMobileNumber1" type="java.lang.String">
			<column name="parentMobileNumber1" />
		</property>
		
		<property name="parentMobileNumber2" type="java.lang.String">
			<column name="parentMobileNumber2" />
		</property>
		
		<property name="emailId" type="java.lang.String">
			<column name="emailId" />
		</property>
		
		<property name="alternateEmailId" type="java.lang.String">
			<column name="alternateEmailId" />
		</property>
		
		<property name="addressLine1" type="java.lang.String">
			<column name="addressLine1" />
		</property>
		
		<property name="addressLine2" type="java.lang.String">
			<column name="addressLine2" />
		</property>
		
		<property name="country" type="java.lang.String">
			<column name="country" />
		</property>
		
		<property name="state" type="java.lang.String">
			<column name="state" />
