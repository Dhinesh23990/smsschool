		<property name="educationalQualification" type="java.lang.String">
			<column name="educationalQualification" />
		</property>
		
		<property name="annualIncome" type="java.lang.String">
			<column name="annualIncome" />
		</property>
		
		<property name="aadharNumber" type="java.lang.String">
			<column name="aadharNumber" />
