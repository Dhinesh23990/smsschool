		</property>
		
		<property name="comAddressLine1" type="java.lang.String">
			<column name="comAddressLine1" />
		</property>
		
		<property name="comAddressLine2" type="java.lang.String">
			<column name="comAddressLine2" />
		</property>
		
		<property name="comCountry" type="java.lang.String">
			<column name="comCountry" />
		</property>
				
		<property name="comState" type="java.lang.String">
			<column name="comState" />
		</property>
		
		<property name="comCity" type="java.util.Date">
			<column name="comCity" />
		</property>
		
		<property name="comPinCode" type="java.lang.String">
			<column name="comPinCode" />
		</property>
		
		<property name="profileImage" type="java.lang.String">
			<column name="profileImage" />
		</property>
		
		<property name="schoolId" type="java.lang.String">
			<column name="schoolId" />
		</property>
		
	</class>
</hibernate-mapping>		   