<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.tgi.sd.domain.SchoolVO" table="school">
		<cache usage="read-write" />

		<id name="id" type="java.lang.String">
			<column name="id" />
		</id>
		<property name="schoolName" type="java.lang.String">
			<column name="schoolName" />
		</property>
		<property name="userName" type="java.lang.String">
			<column name="userName" />
		</property>
		<property name="password" type="java.lang.String">
			<column name="password" />
		</property>
		<property name="schoolCode" type="java.lang.String">
			<column name="schoolCode" />
		</property>
		<property name="address1" type="java.lang.String">
			<column name="address1" />
		</property>
		<property name="address2" type="java.lang.String">
			<column name="address2" />
		</property>
		<property name="city" type="java.lang.String">
			<column name="city" />
		</property>
		<property name="state" type="java.lang.String">
			<column name="state" />
		</property>
		<property name="country" type="java.lang.String">
			<column name="country" />
		</property>
		<property name="zipcode" type="java.lang.String">
			<column name="zipcode" />
		</property>
		<property name="contactEmailId" type="java.lang.String">
			<column name="contactEmailId" />
		</property>
		<property name="contactNumber" type="java.lang.String">
			<column name="contactNumber" />
		</property>
		<property name="status" type="java.lang.String">
			<column name="status" />
		</property>
		<property name="smsTotalCount" type="java.lang.Integer">
			<column name="smsTotalCount" />
		</property>
		<property name="smsBalanceCount" type="java.lang.Integer">
			<column name="smsBalanceCount" />
		</property>
		<property name="smsValidityDays" type="java.lang.Integer">
			<column name="smsValidityDays" />
		</property>
		<property name="smsTotalSentCount" type="java.lang.Integer">
			<column name="smsTotalSentCount" />
		</property>
		<property name="smsTodaySentCount" type="java.lang.Integer">
			<column name="smsTodaySentCount" />
		</property>
		<property name="smsStartDate" type="java.util.Date">
			<column name="smsStartDate" />
		</property>
		<property name="smsEndDate" type="java.util.Date">
			<column name="smsEndDate" />
		</property>

	</class>
</hibernate-mapping>		   