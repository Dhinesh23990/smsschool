<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.tgi.sd.domain.SmsHistoryVO" table="smsHistory">
		<cache usage="read-write"/> 
		
		<id name="id" type="java.lang.String">
			<column name="Id" />
		</id>
		
		<property name="studentId" type="java.lang.String">
		 	<column name="studentId" />
		</property>
		
		<property name="admissionNumber" type="java.lang.String">
		 	<column name="admissionNumber" />
		</property>
		
		<property name="studentName" type="java.lang.String">
			<column name="studentName" />
		</property>
				
		<property name="standard" type="java.lang.String">
			<column name="standard" />
		</property>
		
		<property name="section" type="java.lang.String">
			<column name="section" />
		</property>
		
		<property name="mobileNumber" type="java.lang.String">
			<column name="mobileNumber" />
		</property>
		
		<property name="message" type="java.lang.String">
			<column name="message" />
		</property>
		
		<property name="smsLogId" type="java.lang.String">
			<column name="smsLogId" />
		</property>
		
		<property name="status" type="java.lang.String">
			<column name="status" />
		</property>
		
		<property name="schoolId" type="java.lang.String">
			<column name="schoolId" />
		</property>
		
		
	</class>
</hibernate-mapping>