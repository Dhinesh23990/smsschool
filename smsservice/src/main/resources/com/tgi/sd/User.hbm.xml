<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.tgi.sd.domain.UserVO" table="users">
		<cache usage="read-write" />

		<id name="id" type="java.lang.String">
			<column name="id" />
		</id>
		<property name="userName" type="java.lang.String">
			<column name="userName" />
		</property>
		<property name="password" type="java.lang.String">
			<column name="password" />
		</property>
		<property name="customerId" type="java.lang.String">
			<column name="customerId" />
		</property>
		<property name="lastLogin" type="java.util.Date">
			<column name="lastLogin" />
		</property>
		<property name="contactEmail" type="java.lang.String">
			<column name="contactEmail" />
		</property>
		<property name="roleId" type="java.lang.String">
			<column name="roleId" />
		</property>
		<property name="userStatusCode" type="java.lang.String">
			<column name="userStatusCode" />
		</property>
		
		<property name="schoolId" type="java.lang.String">
			<column name="schoolId" />
		</property>

	</class>
</hibernate-mapping>		   